import pandas as pd
import numpy as np
df=pd.read_csv("HW18__Data.csv",encoding="utf-8")
grouped=df.rename(columns={"行業別":"行業別","經常性薪資-薪資":"經常性","專業人員-薪資":"專業人員","技術員及助理專業人員-薪資":"技術員與助理","事務支援人員-薪資":"事務人員","服務及銷售工作人員-薪資":"服務銷售","技藝_機械設備操作及組裝人員-薪資":"機械設備操作","基層技術工及勞力工-薪資":"基層勞力"})
grouped1=grouped.groupby("行業別")["行業別","經常性","專業人員","技術員與助理","事務人員","服務銷售","機械設備操作","基層勞力"]
print(grouped1.head())
